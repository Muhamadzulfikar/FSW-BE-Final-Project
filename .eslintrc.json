{
  "plugins": ["jest"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/no-dynamic-require": "off",
    "no-path-concat": "off",
    "global-require": "off",
    "prefer-template": "off",
    "strict": "off"
  }
}
