{
  "openapi": "3.0.3",
  "info": {
    "title": "Academy  - OpenAPI 3.0",
    "description": "API documentation for the Academy Website, providing endpoints to manage courses, students, and more\n\nSome useful links:\n- [BackEnd repository](https://github.com/Muhamadzulfikar/FSW-BE-Final-Project)\n- [ERD link](https://dbdiagram.io/d/655ed5293be14957878fc325)\n",
    "license": {
      "name": "ISC License",
      "url": "https://www.isc.org/licenses/"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://final-project-online-course.et.r.appspot.com/"
    }
  ],
  "tags": [
    {
      "name": "Root",
      "description": "Root end-point"
    },
    {
      "name": "Category",
      "description": "Everything about category management"
    },
    {
      "name": "Class",
      "description": "Everything about class management"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "tags": [
          "Root"
        ],
        "summary": "Test your ping to the server here",
        "operationId": "testPing",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ping"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internalError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/course-categories": {
      "get": {
        "tags": [
          "Category"
        ],
        "summary": "Find all Category",
        "description": "This end point will serve all list of categories",
        "operationId": "getAllCategories",
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/categories"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internalError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/courses": {
      "get": {
        "tags": [
          "Class"
        ],
        "summary": "Find all Class if there are no params or spesific class if there are params",
        "description": "This end point will serve all list of class",
        "operationId": "getAllClass",
        "parameters": [
          {
            "name": "categoryId",
            "in": "query",
            "description": "The category of the classes",
            "required": false,
            "schema": {
              "type": "string",
              "example": "1,3"
            }
          },
          {
            "name": "level",
            "in": "query",
            "description": "The level of the classes",
            "required": false,
            "schema": {
              "type": "string",
              "example": "beginner,intermediate"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/classes"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internalError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/course/{id}": {
      "get": {
        "tags": [
          "Class"
        ],
        "summary": "Find detail class by ID",
        "description": "This end point will serve data of detailed class",
        "operationId": "getDetailClass",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of class to fetch",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/paramsID"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/classDetail"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internalError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/admin-courses": {
      "get": {
        "tags": [
          "Class"
        ],
        "summary": "Get Dashboard Admin",
        "description": "This end point will serve data of user's classes",
        "operationId": "getAdminCourses",
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/userCoursePayments"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internalError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/create-courses": {
      "post": {
        "tags": [
          "Class"
        ],
        "summary": "Create Class",
        "description": "This end point will create data of class",
        "operationId": "createAdminCourses",
        "requestBody": {
          "description": "Data of the course to be created",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/reqClass"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/reqClass"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internalError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/update-courses/:id": {
      "put": {
        "tags": [
          "Class"
        ],
        "summary": "Update Class",
        "description": "This end point will update data of class",
        "operationId": "updateAdminCourses",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the course to be updated",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "dataCourse",
            "in": "body",
            "description": "Data of the course to be updated",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/reqClass"
            }
          }
        ],
        "requestBody": {
          "description": "Data of the course to be updated",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/reqClass"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "example": {
                  "status": "OK",
                  "code": 200,
                  "message": "Success"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internalError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/delete-courses/:id": {
      "delete": {
        "tags": [
          "Class"
        ],
        "summary": "Delete Class",
        "description": "This end point will delete data of class",
        "operationId": "deleteAdminCourses",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the course to be updated",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid" 
            }
          }
        ],
        "requestBody": {
          "description": "Data of the course to be deleted",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/reqClass"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "example": {
                  "status": "OK",
                  "code": 200,
                  "message": "Success"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internalError"
                }
              }
            }
          }
        }
      }
    }
  },
